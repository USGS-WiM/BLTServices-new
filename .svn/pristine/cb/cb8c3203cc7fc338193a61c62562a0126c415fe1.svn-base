    $(function () {
        $("#createForm").submit(function () {
            $.ajax({
                url: this.action,
                type: this.method,
                data: $(this).serialize(),
                success: function (result) {
                    if (result) {
                        window.close();
                        top.opener.reload(true); //not sure if this works because testing is another window, not popup within iframe
